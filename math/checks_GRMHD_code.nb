Cell[TextData[{
 "beta = atan2 (yg, xg) - corresponds to the picture\nxpl=b Cos[\[Beta]]; \
ypl=b Sin[\[Beta]];rA Cos[\[Theta]A] == r Cos[\[Theta]0] + ypl Sin[\[Theta]0] \
\[Implies] \[Theta]A=ArcCos",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    FractionBox[
     RowBox[{
      RowBox[{"r0", " ", 
       RowBox[{"Cos", "[", "\[Theta]0", "]"}]}], "+", 
      RowBox[{"ypl", " ", 
       RowBox[{"Sin", "[", "\[Theta]0", "]"}]}]}], "rA"], "]"}], 
   TraditionalForm]]],
 ";\n Tan[\[Phi]A]==xpl/(r Sin[\[Theta]0]-ypl Cos[\[Theta]0]) \[Implies] ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Phi]A", "=", 
    RowBox[{"ArcTan", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"r0", " ", 
        RowBox[{"Sin", "[", "\[Theta]0", "]"}]}], "-", 
       RowBox[{"ypl", " ", 
        RowBox[{"Cos", "[", "\[Theta]0", "]"}]}]}], ",", " ", "xpl"}], 
     "]"}]}], TraditionalForm]]],
 "; \n \nk={0, Sin[\[Theta]A] Cos[\[Phi]A] Sin[\[Theta]0] + Cos[\[Theta]A] \
Cos[\[Theta]0], Cos[\[Theta]A] Cos[\[Phi]A] Sin[\[Theta]0] - Sin[\[Theta]A] \
Cos[\[Theta]0], Sin[\[Phi]A] Sin[\[Theta]0]};\n",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["k", "\[Mu]"], TraditionalForm]]],
 "={kt,1,0,0}; a0={0,0,-1,0}; b={0,0,0,-1}; \nz=r Cos[\[Theta]];\nx=r Sin[\
\[Theta]]Sin[\[Phi]];\ny=r Sin[\[Theta]]Cos[\[Phi]];\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"r", "=", 
      RowBox[{
       SqrtBox[
        RowBox[{
         SuperscriptBox["x", "2"], "+", 
         SuperscriptBox["y", "2"], "+", 
         SuperscriptBox["z", "2"]}]], "\[Implies]", " ", 
       RowBox[{"dr", "\[Equal]", " ", 
        FractionBox[
         RowBox[{"dx", "+", "dy", "+", "dz"}], "r"]}]}]}], ";"}], " "}], 
   TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Theta]", "=", 
    RowBox[{
     RowBox[{"ArcTan", "[", 
      RowBox[{
       SqrtBox[
        RowBox[{
         SuperscriptBox["x", "2"], "+", 
         SuperscriptBox["y", "2"]}]], "/", "z"}], "]"}], " ", "\[Implies]", 
     " ", 
     RowBox[{"d\[Theta]", " ", "\[Equal]"}]}]}], TraditionalForm]]],
 "\nk={kt,0,Sin[\[Theta]],Cos[\[Theta]]}={kt,kx,ky,kz} at a point {x,y,z}=rA{ \
Sin[\[Theta]A]Sin[\[Phi]A], Sin[\[Theta]A]Cos[\[Phi]A],Cos[\[Phi]A]}; line: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"x", "-", "xA"}], "kx"], "\[Equal]", 
    FractionBox[
     RowBox[{"y", "-", "yA"}], "ky"], "\[Equal]", 
    FractionBox[
     RowBox[{"z", "-", "zA"}], "kz"], "\[Equal]", "t"}], TraditionalForm]]],
 ";\n"
}], "Text",
